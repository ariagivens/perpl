{- Test whether a string generated by the grammar

     S -> S S / p
     S -> a   / 1-p

   has odd length or not. -}

extern sample_p: Bool;

define not: Bool->Bool = \b: Bool. if b then False else True;

define odd: Bool->Bool = \q: Bool.
  if sample_p then odd (odd q) else not q;
    
odd False
